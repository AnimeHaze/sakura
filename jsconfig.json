{
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vidstack/vue", "vidstack/globals"],
    "paths": {
      "@/*": [
        "./src/renderer/main-window/*"
      ],
      "@notify/*": [
        "./src/renderer/notify-window/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@enums/*": [
        "./src/enums/*"
      ]
    }
  }
}
